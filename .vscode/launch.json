{
    "configurations": [
        {
            "name": "aiperf",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "module": "aiperf.cli",
            "preLaunchTask": "clean-artifacts",
            "args": [
                "profile",
                "-m",
                "Qwen/Qwen3-0.6B",
                // "--log-level",
                // "DEBUG",
                "--max-workers",
                "1",
                "--request-count",
                "1",
                "--streaming",
                "--slice-duration",
                "1"
            ]
        },
        {
            "name": "pytest",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "module": "pytest",
            "args": [
                "-n",
                "auto"
            ]
        }
    ],
    "version": "0.2.0"
}